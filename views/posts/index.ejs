<!doctype html>
<html lang="ja">

<head>
  <title>Welcome Page</title>
  <%- include('../_share/stylesheets.ejs') %>
</head>

<body>
  <%- include('../_share/navbar.ejs') %>
  
  <main role="main" class="container">

    <div class="border-bottom mt-5 mb-5">
      <h1><%= title %> </h1>
    </div>
    <dl class="row mt-5 mb-5">
      <dt class="col-3">公開日</dt>
      <dd class="col-9"><%= document.published.toLocaleString() %></dd>
      <dt class="col-3">著者</dt>
      <dd class="col-9"><%= document.authors %></dd>
      <dt class="col-3">キーワード</dt>
      <dd class="col-9"><%= document.keywords %></dd>
    </dl>
    
    <p class="lead"> <%= document.content %> </p>

  </main>
  
  <%- include('../_share/javascripts.ejs') %>
</body>

</html>