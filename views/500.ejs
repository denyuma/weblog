<!doctype html>
<html lang="ja">

<head>
  <title>500: Internal Server Error</title>
  <%- include('./_share/stylesheets.ejs') %>
</head>

<body>
  <%- include('./_share/navbar.ejs') %>
  

  <main role="main" class="container">

    <div class="border-bottom mt-5 mb-5">
      <h1>500: Internal Server Error</h1>
      <h2>リクエスト</h2>
      <table class="table table-bordered">
        <tr>
          <th>リクエスト</th>
          <td>
            <span><%= data.method %></span>
            <span>&nbsp;</span>
            <span><%= data.url %></span>
            <span>&nbsp;</span>
            <span><%= (data.protocol || '' ).toUpperCase() %>/<%= data.version %></span>
          </td>
        </tr>
      </table>
      <% if(data.error) { %>
        <h2>エラー</h2>
      <table class="table table-bordered">
        <tr>
          <th>name</th>
          <td><span><%= data.error.name %></span></td>
        </tr>
        <tr>
          <th>message</th>
          <td><span><%= data.error.message %></span></td>
        </tr>
        <tr>
          <th>stack</th>
          <td><span><%= data.error.stack %></span></td>
        </tr>
      </table>
      <% } %>
    </div>

  </main>
  
  <%- include('./_share/javascripts.ejs') %>
</body>

</html>